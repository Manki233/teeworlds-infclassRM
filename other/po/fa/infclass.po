#: src/game/server/gamemodes/mod.cpp:298 src/game/server/gamemodes/mod.cpp:405
msgid "Infected won the round in {sec:RoundDuration}"
msgstr "زامبی ها برنده این راند شدند در {sec:RoundDuration}"

#: src/game/server/gamemodes/mod.cpp:385
msgid "One human won the round"
msgid_plural "{int:NumHumans} humans won the round"
msgstr[0] "{int:NumHumans} انسان ها برنده راند شدند"
msgstr[1] "{int:NumHumans} انسان برنده شد(ند)"

#: src/game/server/gamemodes/mod.cpp:398
msgid "You have survived, +5 points"
msgstr "شما توانستید جان سالم به در ببرید، +5 امتیاز"

#: src/game/server/gamemodes/mod.cpp:546
#: src/game/server/entities/character.cpp:2471
