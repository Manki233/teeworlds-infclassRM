msgstr "{str:Victim} توانایی صحبت را به مدت {sec:Duration} به دلیل ({str:Reason}) از دست داد"

#: src/engine/server/server.cpp:2912
msgid "You are now logged."
msgstr "شما وارد شدید!"

#: src/engine/server/server.cpp:2919
