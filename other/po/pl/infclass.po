msgid_plural "{int:NumHumans} humans won the round"
msgstr[0] "Jeden człowiek wygrał rundę"
msgstr[1] "{int:NumHumans} Ludzie wygrali rundę"
msgstr[2] "{int:NumHumans} Ludzie wygrali rundę "

#: src/game/server/gamemodes/mod.cpp:398
