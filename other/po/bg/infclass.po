#: src/engine/server/server.cpp:2164
msgid "{str:Victim} has been muted for {sec:Duration} ({str:Reason})"
msgstr "{str:Victim} е заглушен за {sec:Duration} ({str:Reason})"

#: src/engine/server/server.cpp:2912
msgid "You are now logged."
msgstr "Успешно влязохте в акаунта си."

#: src/engine/server/server.cpp:2919
msgid "Wrong username/password."
msgstr "Грешен юзер/парола."

#: src/engine/server/server.cpp:2994
msgid "An error occured during the operation."
msgstr "Имаше грешка по време на операцията."

#: src/engine/server/server.cpp:3009
msgid "You must be logged for this operation"
msgstr "Трябва да се влезли за да изпълните тази операция"

#: src/engine/server/server.cpp:3063
msgid "Please wait 5 minutes before creating another account"
msgstr "Моля, изчакайте 5 минути преди да създадете друг акаунт"

#: src/engine/server/server.cpp:3071 src/engine/server/server.cpp:3098
#: src/engine/server/server.cpp:3117 src/engine/server/server.cpp:3156
#: src/engine/server/server.cpp:3164
msgid "An error occured during the creation of your account."
msgstr "Възникна грешка по време на създаването на вашия акаунт."

#: src/engine/server/server.cpp:3090
msgid "This username is already taken by an existing account"
msgstr "Това потребителско име вече е заето"

#: src/engine/server/server.cpp:3135
msgid "Your account has been created and you are now logged."
msgstr "Вашият акаунт беше създаден, и сега сте влезли."

#: src/engine/server/server.cpp:3719
msgid "You must be logged to see your rank"
msgstr "Трябва да сте логнати за да видите вашия ранг"

#: src/engine/server/server.cpp:3803
msgid "You must be logged to see your goal"
msgstr "Трябва да сте логнати за да видите вашата цел"

#: src/engine/server/server.cpp:4110
msgid "You have already notified that {str:PlayerName} ought to be banned"
msgstr "Вие вече съобщихте, че {str:PlayerName} трябва да бъде баннат"

#: src/engine/server/server.cpp:4125
msgid "{str:PlayerName} wants {str:VictimName} to be banned ({str:Reason})"
msgstr "{str:PlayerName} иска {str:VictimName} да бъде баннат ({str:Reason})"

#: src/game/server/gamemodes/mod.cpp:243 src/game/server/gamemodes/mod.cpp:267
msgid "{str:VictimName} has been infected"
msgstr "{str:VictimName} беше инфектиран"

#: src/game/server/gamemodes/mod.cpp:298 src/game/server/gamemodes/mod.cpp:405
msgid "Infected won the round in {sec:RoundDuration}"
msgstr "Инфектираните спечелиха рунда за {sec:RoundDuration}"

#: src/game/server/gamemodes/mod.cpp:385
msgid "One human won the round"
msgid_plural "{int:NumHumans} humans won the round"
msgstr[0] "Един човек спечели рунда"
msgstr[1] "{int:NumHumans} хора спечелиха рунда"

#: src/game/server/gamemodes/mod.cpp:398
msgid "You have survived, +5 points"
msgstr "Оцеляхте, +5 точки"

#: src/game/server/gamemodes/mod.cpp:546
#: src/game/server/entities/character.cpp:2471
msgid "You have infected {str:VictimName}, +3 points"
msgstr "Вие заразихте {str:VictimName}, +3 точки"

#: src/game/server/gamemodes/mod.cpp:576
msgid "You have killed a witch, +5 points"
msgstr "Вие убихте вещица, +5 точки"

#: src/game/server/gamemodes/mod.cpp:588 src/game/server/gamemodes/mod.cpp:614
msgid "You have eliminated your target, +2 points"
msgstr "Вие елиминирахте вашата цел, +2 точки"

